#rofi
alt + F2
	rofi -show drun -lines 10 -width 40

super + w
	rofi -show window -width 80

#terminal
alt + t
	termite

#file manager
super + e
	pcmanfm

#Screenshot
Print
	flameshot gui

#Email client
XF86Mail
	thunderbird

#Power menu
alt + ctrl + Delete
	$HOME/.scripts/power.sh
	
# Center window
shift + alt + c
	xdotool getactivewindow windowsize 960 540 windowmove 500 200

# Move window to left
shift + super + Left
	/usr/local/bin/movescreen.py left

# Move window to right
shift + super + Right
	/usr/local/bin/movescreen.py right
